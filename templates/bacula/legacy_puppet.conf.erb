Client {
    Name           = pdx-wl-logmon.int.codeaurora.org-fd
    Address        = pdx-wl-logmon.int.codeaurora.org 
    FDPort         = 9102
    Catalog        = MyCatalog
    Password       = "<%= @client_password %>"
    File Retention = 45 days
    Job Retention  = 6 months
    AutoPrune      = yes

}

FileSet {
  Name = pdx-wl-logmon.int.codeaurora.org-etc
  Include {
    Options {
      signature = MD5
      compression = GZIP
    }
    File = /etc
  }
  Exclude {
  }
}

